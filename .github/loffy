    """snos channels"""

    strings={"name": "channel riper"}

    async def clearcmd(self, message: Message):

            chat_id = message.chat.id

            messages = get_chat_history(chat_id)

            messages = client.get_messages(chat_history)
    for m in messages:

        try:

