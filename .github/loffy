import telebot

bot = telebot.TeleBot("YOUR_TOKEN_HERE") # здесь нужно заменить YOUR_TOKEN_HERE на токен вашего бота

@bot.message_handler(commands=['clear'])
def clear_channel(message):
    chat_id = message.chat.id
    messages = bot.get_chat_history(chat_id)
    for m in messages:
        try:
            bot.delete_message(chat_id, m.id)
        except:
            pass
    bot.reply_to(message, "Канал был очищен от сообщений.")

bot.polling()
